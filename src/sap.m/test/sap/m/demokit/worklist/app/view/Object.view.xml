<mvc:View
	controllerName="sap.ui.demo.worklist.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		id="page"
		navButtonPress="onNavBack"
		showNavButton="true"
		title="{i18n>objectTitle}">
		<content>
			<ObjectHeader
				id="objectHeader"
				title="{Name}"
				number="{UnitNumber}"
				numberUnit="{UnitOfMeasure}">
			</ObjectHeader>
		</content>
		<footer>
			<Toolbar>
			</Toolbar>
		</footer>
	</Page>
</mvc:View>
